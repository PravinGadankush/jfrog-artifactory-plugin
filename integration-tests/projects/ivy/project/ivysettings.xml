<?xml version="1.0" encoding="UTF-8"?>
<ivy-settings>
  <settings defaultResolver="main" />
  <!--Authentication required for publishing (deployment). 'Artifactory Realm' is the realm used by Artifactory so don't change it.-->
  <credentials host="nginx" realm="Artifactory Realm" username="admin" passwd="password" />
  <resolvers>
    <chain name="main">
      <ibiblio name="public" m2compatible="true" root="https://nginx:443/artifactory/ivy-remote" />
    </chain>
  </resolvers>
</ivy-settings>
